@model Areas.Models.UserAddressDetails

<ul id="ulAddressLabels">
    <li><strong>Address Line 1 </strong> <p> : @Model.Address1</p>  </li>
    <li><strong>Address Line 2 </strong> <p> : @Model.Address2</p>  </li>
    <li><strong>City </strong> <p> : @Model.CityName</p>  </li>
    <li><strong>State </strong> <p> : @Model.StateName </p>  </li>
    <li><strong>Country </strong> <p>:  @Model.CountryName</p>  </li>
    <li><strong>Pincode </strong> <p>:  @Model.Pincode</p>  </li>

</ul>

<ul id="ulAddressEdit" style="display:none;">
    <li>
        <strong>Address1 </strong>:
            @Html.TextBoxFor(m => m.Address1, htmlAttributes: new { @id = "txtAddress1", @placeholder = "Enter Address", })
    </li>
    <li>
        <strong>Field</strong>:
        @Html.TextBoxFor(m => m.Address2, htmlAttributes: new { @id = "txtAddress2", @placeholder = "Enter Address", })
    </li>
    <li>
        <strong>City</strong>:
        @Html.DropDownListFor(m => m.CityId, new SelectList(Model.CityDetails, "Value", "Text"), htmlAttributes: new { @id="ddlCity"})
    </li>
    <li>
        <strong>State</strong>:
        @Html.DropDownListFor(m => m.StateId, new SelectList(Model.StateDetails, "Value", "Text"), htmlAttributes: new { @id="ddlState"})
    </li>
    <li>
        <strong>Country</strong>:
        @Html.DropDownListFor(m => m.CountryId, new SelectList(Model.CountryDetails, "Value", "Text"), htmlAttributes: new { @id = "ddlCountry" })
    </li>
    <li>
        <strong>Income</strong>:
        @Html.TextBoxFor(m => m.Pincode, htmlAttributes: new { @id = "txtPincode", @placeholder = "Enter Pincodes" })
    </li>
</ul>

